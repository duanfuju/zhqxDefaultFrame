.slideBox{ width:352px; margin-top:10px; height:188px; overflow:hidden; position:relative; border:1px solid #ddd;  }
.slideBox .hd{ height:15px; overflow:hidden; position:absolute; right:5px; bottom:5px; z-index:1; }
.slideBox .hd ul{ overflow:hidden; zoom:1; float:left;  }
.slideBox .hd ul li{ float:left; margin-right:2px;  width:15px; height:15px; line-height:14px; text-align:center; background:#fff; cursor:pointer; }
.slideBox .hd ul li.on{ background:#f00; color:#fff; }
.slideBox .bd{ position:relative; height:180px; z-index:0;   }
.slideBox .bd li{ zoom:1; vertical-align:middle; }
.slideBox .bd img{ width:250px; height:auto; display:block;  margin:3px auto; }
.slideBox .bd .slidecon{ width:250px; height:39px; display:block;  margin:0 auto; font-size:14px; color: #989898; }
.slideBox .bd .slidecon span{ color:#0055c2; display:block;  font-size:16px;  }
.slideBox .bd .slidecon font{color:#7b7b7b;padding-left:10px}
.slideBox .bd .slidecon label{ color:Black; float:left; }
.slideBox .bd .slidecon div{ float:right;}
.slideBox .bd .slidecon div.ws{width:170px;float:left;font-size:12px}



/* 下面是前/后按钮代码，如果不需要删除即可 */
.slideBox .prev{ position:absolute;left:3%; top:30px; display:block; width:38px; height:137px; background:url(/images/Scenery_l.png) 0 -139px no-repeat;}
.slideBox .prev:hover{background-position:0 0; }
.slideBox .next{ position:absolute;right:3%; top:30px; display:block; width:38px; height:137px; background:url(/images/Scenery_r.png) 0 0 no-repeat;}
.slideBox .next:hover{ background-position:0 -139px; }

.slideBox .prevStop{ display:none;  }
.slideBox .nextStop{ display:none;  }


